{
  "name": "v3",
  "version": "3.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev --experimental-https",
    "build": "npm run prisma:generate && npm run build:sitemap && npm run build:robots && next build",
    "analyze": "ANALYZE=true npm run build",
    "start": "next start",
    "lint": "next lint --fix",
    "test": "jest --watch",
    "test:ci": "jest --ci --coverage",
    "prisma:generate": "prisma generate --schema=./src/prisma/schema.prisma",
    "build:sitemap": "ts-node scripts/generate-sitemap.mjs",
    "build:robots": "ts-node scripts/generate-robots-txt.mjs",
    "start:test:local": "npm run prisma:generate && npm run dev",
    "start:test:ci": "npm run compose:up && npm run build && npm run start",
    "compose:up": "docker-compose up -d",
    "compose:down": "docker-compose down",
    "submodule-install": "./vercel-submodule-workaround.sh && yarn --ignore-scripts --production=false"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.4.0",
    "@prisma/client": "^5.8.1",
    "@vercel/analytics": "^1.1.1",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.4.1",
    "next": "14.1.0",
    "next-auth": "^5.0.0-beta.11",
    "next-intl": "^3.5.2",
    "next-themes": "^0.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.0.1",
    "server-only": "^0.0.1",
    "valibot": "^0.27.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.8.5",
    "@next/bundle-analyzer": "^14.0.4",
    "@playwright/test": "^1.41.1",
    "@testing-library/dom": "^9.3.4",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^29.5.11",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "^20.11.4",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.19",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "axe-core": "^4.8.3",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-next": "^14.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.6.3",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^6.2.0",
    "jest": "^29.7.0",
    "jest-axe": "^8.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss-nesting": "^12.0.2",
    "prettier": "3.2.2",
    "prisma": "^5.8.1",
    "react-test-renderer": "^18.2.0",
    "sitemap": "^7.1.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
